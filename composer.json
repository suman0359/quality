{
    "name": "suman0359/quality",
    "description": "Endroid Quality",
    "keywords": ["endroid", "quality","dev"],
    "homepage": "https://github.com/suman0-359/quality",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tasfir Hossain Suman",
            "email": "tasfir@webtech-bd.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "symfony/var-dumper": "^5.4||^6.0"
    },
    "suggest": {
        "roave/security-advisories": "Avoids installation of package versions with vulnerabilities"
    },
    "bin": [
        "bin/code-quality",
        "bin/check",
        "bin/unit-test",
        "bin/functional-test",
        "bin/test"
    ],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
